#!/usr/bin/env ruby

require 'devpki'
require 'devpki/data_directory'

if not DevPKI::DataDirectory::platform_supported?
  puts "ERROR: currently only #{DevPKI::DataDirectory.supported_operating_systems} is supported. You are running on #{DevPKI::DataDirectory.os}. The application will now terminate."
  abort
end
puts DevPKI::DataDirectory.absolute_path
#DevPKI::CLI.start

